<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.GetManagerGreenSpaceController">
    <Label fx:id="welcomeLabel" layoutX="52.0" layoutY="44.0" style="-fx-font-size: 18px; -fx-text-fill: #333;" text="Green Spaces Managed By Me" />
    <TextField fx:id="nameField" blendMode="SRC_ATOP" layoutX="60.0" layoutY="90.0" promptText="Manager Name:" style="-fx-font-size: 14px; -fx-padding: 10; -fx-background-radius: 5; -fx-border-color: #ccc; -fx-border-radius: 5;" />
    <Button fx:id="loadGreenSpacesButton" layoutX="60.0" layoutY="150.0" onAction="#loadGreenSpaces" prefHeight="42.0" prefWidth="180.0" style="-fx-font-size: 14px; -fx-padding: 10; -fx-background-radius: 5; -fx-border-color: #ccc; -fx-border-radius: 5;" text="Load Green Spaces" />
    <ListView fx:id="listOfGreenSpaces" layoutX="300.0" layoutY="50.0" prefHeight="300.0" prefWidth="250.0" />
    <Button fx:id="backButton" layoutX="60.0" layoutY="350.0" onAction="#goBack" style="-fx-font-size: 14px; -fx-padding: 10; -fx-background-radius: 5; -fx-border-color: #ccc; -fx-border-radius: 5;" text="Back" />
</AnchorPane>

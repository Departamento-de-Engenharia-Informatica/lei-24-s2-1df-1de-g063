<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.RegisterSkillPageController">

    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <Label layoutX="14.0" layoutY="14.0" text="Register Skill" />
    <TextField fx:id="skillNameField" layoutX="14.0" layoutY="40.0" promptText="Enter skill name" />
    <Button layoutX="14.0" layoutY="70.0" onAction="#submitData" text="Submit" />
    <ListView fx:id="skillListView" layoutX="14.0" layoutY="100.0" prefHeight="150.0" prefWidth="200.0" />
    <Label fx:id="statusLabel" layoutX="14.0" layoutY="270.0" />


</AnchorPane>
